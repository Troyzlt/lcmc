--- global_common.conf ---
## generated by drbd-gui

global {
		usage-count	yes;
}

common {
	proxy {
		memlimit	100M;
		plugin {
			zlib level 9;
		}
	}

	startup {
		wfc-timeout	3;
		degr-wfc-timeout	0;
	}

	net {
		protocol	A;
		cram-hmac-alg	sha1;
		shared-secret	SECRET;
		ping-timeout	100;
	}

	disk {
		on-io-error	detach;
	}

}
