--- r0.res ---
resource r0 {
	handlers {
		fence-peer	"LIBDIR/drbd-peer-outdater -t 5";
	}

	startup {
		wfc-timeout	9;
	}

	net {
		protocol	B;
		max-epoch-size	5k;
	}

	disk {
		resync-after	r1;
	}

	on host1 {
		volume 0 {
			device		/dev/drbd0;
			disk		DISK;
			flexible-meta-disk	internal;
		}
		address		127.0.0.1:7788;
		proxy on host1 {
			inside 127.0.0.1:7789;
			outside IP:7788;
		}
	}
	on host2 {
		volume 0 {
