## generated by drbd-gui

global {
		usage-count	yes;
}

common {
	proxy {
		memlimit	100M;
		plugin {
			zlib level 9;
		}
	}

	startup {
		degr-wfc-timeout	0;
	}

	net {
		cram-hmac-alg	sha1;
		shared-secret	SECRET;
	}

	disk {
		on-io-error	detach;
	}

}

resource r0 {
	on host1 {
		volume 0 {
			device		/dev/drbd0;
			disk		DISK;
			flexible-meta-disk	internal;
		}
		address		127.0.0.1:7789;
		proxy on host1 {
			inside 127.0.0.1:7789;
			outside IP:7788;
                }
	}
	on host2 {
		volume 0 {
			device		/dev/drbd0;
			disk		DISK;
			flexible-meta-disk	internal;
		}
		address		127.0.0.1:7789;
		proxy on host2 {
			inside 127.0.0.1:7789;
			outside IP:7788;
                }
	}
}
