## generated by drbd-gui

global {
		usage-count	yes;
}

common {
	startup {
		degr-wfc-timeout	0;
	}

	net {
		cram-hmac-alg	sha1;
		shared-secret	SECRET;
	}

	disk {
		on-io-error	detach;
	}

}

resource r0 {
	protocol	C;

	on host1 {
		device	/dev/drbd0;
		disk	DISK;
		address	IP:7788;
		flexible-meta-disk	internal;
	}

	on host2 {
		device	/dev/drbd0;
		disk	DISK;
		address	IP:7788;
		flexible-meta-disk	internal;
	}
}
