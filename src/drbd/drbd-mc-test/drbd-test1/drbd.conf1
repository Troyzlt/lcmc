global {
		usage-count	yes;
}

resource r0 {
	protocol	C;

	startup {
		degr-wfc-timeout	0;
	}

	net {
		cram-hmac-alg	sha1;
		shared-secret	SECRET;
	}

	disk {
		on-io-error	detach;
	}


