		device	/dev/drbd0;
		disk	DISK;
		address	IP:7788;
		flexible-meta-disk	internal;
	}

	on host2 {
		device	/dev/drbd0;
		disk	DISK;
		address	IP:7788;
		flexible-meta-disk	internal;
	}
